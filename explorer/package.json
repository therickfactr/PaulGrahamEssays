{
  "name": "explorer",
  "version": "1.0.0",
  "description": "Paul Graham Essays Explorer - Combined API and Frontend",
  "private": true,
  "scripts": {
    "dev:api": "ts-node-dev --respawn --transpile-only src/api/app.ts",
    "dev:app": "next dev -p 3080",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:app\"",
    "build:api": "tsc && webpack --config webpack.config.js",
    "build:app": "next build",
    "build": "npm run build:api && npm run build:app",
    "start:api": "node dist/api/app.js",
    "start:app": "next start -p 3080",
    "start": "concurrently \"npm run start:api\" \"npm run start:app\"",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "lint": "next lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@langchain/community": "^0.3.38",
    "@langchain/core": "^0.3.43",
    "@langchain/openai": "^0.5.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@supabase/supabase-js": "^2.49.4",
    "bufferutil": "^4.0.9",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "geist": "^1.3.1",
    "lucide-react": "^0.294.0",
    "next": "14.2.26",
    "openapi": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tw-animate-css": "^1.2.5",
    "utf-8-validate": "^6.0.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.21",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.0.4",
    "langchain": "^0.3.19",
    "null-loader": "^4.0.1",
    "path-browserify": "^1.0.1",
    "postcss": "^8.5.3",
    "rimraf": "^6.0.1",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^4.1.1",
    "ts-loader": "^9.5.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2",
    "util": "^0.12.5",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
